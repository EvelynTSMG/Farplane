<UserControl x:Class="Farplane.FFX.EditorPanels.SkillEditorPanel.SkillEditorPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.SkillEditorPanel"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <GroupBox Header="Skills">
        <TabControl>
            <TabItem FontSize="16" Header="Commands">
                <local:CommandEditor x:Name="CommandEditor" />
            </TabItem>
        </TabControl>
    </GroupBox>
</UserControl>
